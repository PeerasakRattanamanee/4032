generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User_Role {
  role   Role @relation(fields: [roleId], references: [id])
  roleId Int
  user   User @relation(fields: [userId], references: [id])
  userId Int

  @@id([roleId, userId])
}

model User {
  id        Int         @id @default(autoincrement())
  firstname String
  User_Role User_Role[]
}

model Role {
  id        Int         @id @default(autoincrement())
  name      String
  User_Role User_Role[]
}
